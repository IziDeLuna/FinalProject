<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>New Project</title>
</head>
<body>
<h3>Add Employee</h3>
<%= form_for :add_pro, url: "/employees/add_pro_create" do |f| %>
    <p>
      <%=f.label :pname %><br>
      <%= f.text_area :pname %>
    </p>
    <p>
      <%=f.label :start_date %><br>
      <%= f.text_area :start_date %>
    </p>
    <p>
      <%=f.label :end_date %><br>
      <%= f.text_area :end_date %>
    </p>
    <p>
      <%=f.label :location %><br>
      <%= f.text_area :location %>
    </p>
    <p>
      <%= f.submit "Add Project" %>
    </p>
<% end %>
</body>
</html>




